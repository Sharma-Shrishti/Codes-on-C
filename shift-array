#include <stdio.h>
int fun(int books[],int demand[])
{
    int i=0,j=0,k=0;
    int flag=0,pos=0;
    int result[10];
    while(demand[i]!=-1)
    {
        flag=demand[i];
        printf("flag=  %d\n  ",flag);
        while(books[j]!=-1)
        {
            if(books[j]==flag)
            {
                result[k++]=pos+1;
                //pos=0;
                books[j]=0;
                break;
            }
            else if(books[j]!=0)
            {
                pos=pos+1;
            }
        j++;
        }
    i++;
    j=0;
    pos=0;
    }
    for(i=0;i<k;i++)
    {
        printf("%d\n",result[i]);
    }
}
int main()
{
    printf("Hello, World!\n");
    int books[]={23,56,78,12,76,-1};
    int demand[]={78,23,12,-1};
    fun(books,demand);
    return 0;
}
