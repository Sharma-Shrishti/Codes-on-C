#include <stdio.h>

int atoi(char *st)
{
    int i;
    int result=0;
    for(i=0;st[i]!='\0';++i)
    {
        if(st[i]>='0' && st[i]<='9')
        result=result*10+st[i]-'0';
        
    }
    return result;
}
int main()
{
    //printf("Hello, World!\n");
    char st[]="g";
    int val=atoi(st);
    printf("%d",val);
    return 0;
}
